    "from math import sin, cos, tan, exp, pi\n",
    "def grad_1(x, y, z):\n",
    "    #возвращает кортеж из 3 чисел --- частных производных по x,y,z \n",
    "    dx = z * cos(x * z) + exp(x)\n",
    "    dy = -2 * y * z\n",
    "    dz = x * cos(x * z) - y * y\n",
    "    return (dx, dy, dz)"
