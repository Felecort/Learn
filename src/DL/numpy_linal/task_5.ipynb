   "source": [
    "def transform(X, a=1):\n",
    "    Y = X.copy()\n",
    "    Y[:, 1::2] = a\n",
    "    Y[:, ::2] **= 3\n",
    "    return np.hstack((X, Y[:, ::-1]))"
   ]
  },
